{
  "name": "hoc-token",
  "version": "1.0.0",
  "description": "A modified ERC-721 contract that represents home ownership.",
  "scripts": {
    "compile": "truffle compile",
    "compile:all": "truffle compile --all",
    "migrate:development": "truffle migrate --network development",
    "migrate:goerli": "truffle migrate --network goerli",
    "migrate:goerli:reset": "truffle migrate --network goerli --reset",
    "migrate:dashboard": "truffle migrate --network dashboard",
    "migrate:dashboard:reset": "truffle migrate --network dashboard --reset",
    "test": "truffle test",
    "dashboard": "truffle dashboard",
    "verify:home-onchain-token:goerli": "truffle run verify HomeOnChainToken --network goerli",
    "verify:roofstock-onchain-kyc:goerli": "truffle run verify RoofstockOnChainKyc --network goerli",
    "verify:home-onchain-token:mainnet": "truffle run verify HomeOnChainToken --network mainnet",
    "verify:roofstock-onchain-kyc:mainnet": "truffle run verify RoofstockOnChainKyc --network mainnet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RoofstockOnChain/hoc-token.git"
  },
  "keywords": [
    "roofstock",
    "truffle",
    "smart-contract",
    "ERC-721"
  ],
  "author": "Roofstock onChain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RoofstockOnChain/hoc-token/issues"
  },
  "homepage": "https://github.com/RoofstockOnChain/hoc-token#readme",
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.2",
    "@openzeppelin/contracts-upgradeable": "^4.9.2",
    "@openzeppelin/truffle-upgrades": "^1.19.0",
    "@quadrata/contracts": "^1.0.3",
    "@truffle/hdwallet-provider": "^2.1.13",
    "dotenv": "^16.3.1",
    "truffle": "^5.11.1",
    "truffle-plugin-verify": "^0.5.33"
  }
}
